{
    "name": "dnd-markdown",
    "version": "1.2.0",
    "description": "",
    "bin": {
        "dnd-markdown": "parse.js"
    },
    "pkg": {
        "assets": [
            "node_modules/**/*",
            "parse.js"
        ],
        "targets": [
            "node12-macos-x64",
            "node12-win-x64"
        ]
    },
    "scripts": {
        "test": "node parse.js --debug test/main.md -o dist/bundle_t.md",
        "main": "node parse.js document/main.md -o dist/bundle.md",
        "build": "pkg --output bin/mdparse .",
        "clear": "rm -rf dist"
    },
    "keywords": [],
    "author": "blitzher",
    "license": "MIT",
    "dependencies": {
        "argparse": "^2.0.1"
    }
}
